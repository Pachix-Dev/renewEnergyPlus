---
import { MarqueeReact } from '../components/MarqueeReact'
import {
  exhibitors1,
  exhibitors2,
  exhibitors3,
  exhibitors4,
  exhibitors5,
  exhibitors6,
} from '../data/constanst_logos'
import { t } from 'i18next'
---

<section class=''>
  <p class='text-[#565078] text-5xl text-center font-bold p-10'>
    {t('menu.submenu_2.title')} 2025
  </p>
  <MarqueeReact images={exhibitors1} client:only='react' />
  <MarqueeReact images={exhibitors2} , direction='left' client:only='react' />
  <MarqueeReact images={exhibitors3} client:only='react' />
  <MarqueeReact images={exhibitors4} , direction='left' client:only='react' />
  <MarqueeReact images={exhibitors5} client:only='react' />
  <MarqueeReact images={exhibitors6} , direction='left' client:only='react' />
</section>
